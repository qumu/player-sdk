---
import { SdkEventMessage } from '../../src/models/internal';

type Props = {
  checked: boolean;
  event: SdkEventMessage['name'];
  id: string;
};

const { checked, event }: Props = Astro.props;
---

<div>
  <label>
    <input
        type="checkbox"
        data-type="event"
        data-event={event}
        checked={checked}
    />
    <slot/>
  </label>
</div>
