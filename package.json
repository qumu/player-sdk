{
  "name": "@enghouse-qumu/player-sdk",
  "version": "0.0.8",
  "description": "JavaScript SDK to interact with the player ",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "unpkg": "dist/index.umd.js",
  "module": "./dist/index.module.js",
  "exports": {
    "require": "./dist/index.js",
    "default": "./dist/index.modern.mjs"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "microbundle",
    "build:docs": "npm run build && astro build",
    "dev": "microbundle watch",
    "lint": "eslint 'src/**'",
    "serve": "concurrently \"npm run dev\" \"astro dev\"",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qumu/player-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/qumu/player-sdk/issues"
  },
  "homepage": "https://qumu.github.io/player-sdk",
  "devDependencies": {
    "@astrojs/markdown-remark": "2.1.0",
    "@astrojs/mdx": "0.18.0",
    "@qumu/code-conventions": "2.0.5",
    "@qumu/ui-kit": "0.33.1",
    "@types/jest": "29.4.0",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "astro": "2.1.2",
    "concurrently": "7.6.0",
    "eslint": "8.35.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-jsdoc": "40.0.1",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "microbundle": "0.15.1",
    "rehype-autolink-headings": "6.1.1",
    "sass": "1.58.3",
    "stylelint": "13.9.0",
    "ts-jest": "29.0.5",
    "typescript": "4.9.5"
  }
}
